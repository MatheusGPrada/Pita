{
  "name": "pita",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ],
    "nohoist": []
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "yarn env:staging && yarn bundle:android && cd android && ./gradlew clean assembleRelease -x bundleReleaseJsAndAssets",
    "build:ios": "yarn env:staging && react-native bundle --entry-file packages/app/index.tsx --bundle-output ios/main.jsbundle --dev=false --platform='ios' --assets-dest ios/"
  },
  "dependencies": {
    "@brazilian-utils/brazilian-utils": "^1.0.0-rc.12",
    "@react-native-async-storage/async-storage": "^1.15.9",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "date-fns": "^2.25.0",
    "hoist-non-react-statics": "^3.3.2",
    "i18next": "^19.8.7",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-appearance": "^0.3.4",
    "react-native-cache": "^2.0.1",
    "react-native-cli": "^2.0.1",
    "react-native-gesture-handler": "^1.10.1",
    "react-native-masked-text": "^1.13.0",
    "react-native-paper": "^4.7.2",
    "react-native-popup-menu": "^0.15.10",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^2.17.1",
    "react-native-stepper-ui": "^0.0.9",
    "react-native-svg": "^12.1.0",
    "react-native-vector-icons": "^8.0.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^6.7.1",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "jest": "^27.0.4",
    "metro-react-native-babel-preset": "^0.65.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "16.13.1",
    "tsconfig-paths": "^3.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
